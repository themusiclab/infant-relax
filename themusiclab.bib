@article{Soley2015,
  title = {Infants Prefer Tunes Previously Introduced by Speakers of Their Native Language},
  author = {Soley, Gaye and Sebastián-Gallés, Núria},
  date = {2015-11-01},
  journaltitle = {Child Development},
  shortjournal = {Child Dev.},
  volume = {86},
