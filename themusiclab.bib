}

@article{Hish2019,
  title = {Applying the {{Stress Process Model}} to {{Stress}}–{{Burnout}} and {{Stress}}–{{Depression Relationships}} in {{Biomedical Doctoral Students}}: {{A Cross}}-{{Sectional Pilot Study}}},
  shorttitle = {Applying the {{Stress Process Model}} to {{Stress}}–{{Burnout}} and {{Stress}}–{{Depression Relationships}} in {{Biomedical Doctoral Students}}},
  author = {Hish, Alexander J. and Nagy, Gabriela A. and Fang, Caitlin M. and Kelley, Lisalynn and Nicchitta, Christopher V. and Dzirasa, Kafui and Rosenthal, M. Zachary},
  date = {2019-10-17},
  journaltitle = {CBE—Life Sciences Education},
  shortjournal = {CBE—Life Sci. Educ.},
