}

@article{Groarke2019,
  title = {Listening to Self-Chosen Music Regulates Induced Negative Affect for Both Younger and Older Adults},
  author = {Groarke, Jenny M. and Hogan, Michael J.},
  date = {2019-06-06},
  journaltitle = {PLOS ONE},
  shortjournal = {PLOS ONE},
