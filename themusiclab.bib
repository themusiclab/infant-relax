@article{Galeotti2005,
  title = {Female Preference for Fast-Rate, High-Pitched Calls in {{Hermann}}'s Tortoises {{Testudo}} Hermanni},
  author = {Galeotti, Paolo and Sacchi, Roberto and Rosa, Daniele Pellitteri and Fasola, Mauro},
  date = {2005-01-01},
  journaltitle = {Behavioral Ecology},
  shortjournal = {Behavioral Ecology},
  volume = {16},
  pages = {301--308},
  issn = {1045-2249, 1465-7279},
  doi = {10.1093/beheco/arh165},
  url = {http://beheco.oxfordjournals.org/content/16/1/301},
  urldate = {2015-12-26},
  abstract = {The courtship and mounting behavior of promiscuous tortoises is based on a multiple signaling system that includes visual, olfactory, tactile, and acoustic signals. Vocalizations related to mounting seem to be particularly intriguing because tortoises vocalize mainly at this time. Vocalizations and courtship behavior might be costly for males, and if these costs increase differentially for different males, then the potential exists for vocalizations and displays to reveal male individual quality on which females could base their mate choice. In the present study, by using individuals breeding in seminatural enclosures, we experimentally investigated whether the mounting call of male Hermann's tortoises (Testudo hermanni) represents a relevant stimulus to both males and females. We also looked for potential preference toward different acoustic stimuli in order to determine which call features, if any, played a role in attracting potential partners or deterring rivals. Females were sensitive to call playback, whereas males apparently paid little attention to playback. In addition, females showed a significant preference toward high-pitched calls emitted at high rate. Also, females exhibited a strong tendency toward short-duration calls. Because call rate is related to hematocrit in Hermann's tortoise males, by responding to fast-rate calls females are also choosing high-quality partners as sires of their offspring. This is the first study in which an attractive function of tortoise vocalizations has been documented.},
  keywords = {added,call features,calling rate,mate choice,playback experiments,Testudo hermanni,tortoise},
  langid = {english},
  number = {1}
}

@article{Gangestad2016,
  title = {How Valid Are Assessments of Conception Probability in Ovulatory Cycle Research? {{Evaluations}}, Recommendations, and Theoretical Implications},
  shorttitle = {How Valid Are Assessments of Conception Probability in Ovulatory Cycle Research?},
  author = {Gangestad, Steven W. and Haselton, Martie G. and Welling, Lisa L. M. and Gildersleeve, Kelly and Pillsworth, Elizabeth G. and Burriss, Robert and Larson, Christina M. and Puts, David A.},
  date = {2016-03},
  journaltitle = {Evolution and Human Behavior},
  shortjournal = {Evol. Hum. Behav.},
  volume = {37},
  pages = {85--96},
  issn = {1090-5138},
  doi = {http://dx.doi.org/10.1016/j.evolhumbehav.2015.09.001},
  url = {http://dspace.stir.ac.uk/handle/1893/26503},
  urldate = {2019-09-11},
  abstract = {Over the past two decades, a large literature examining psychological changes across women's ovulatory cycles has accumulated, emphasizing comparisons between fertile and non-fertile phases of the cycle. While some studies have verified ovulation using luteinizing hormone (LH) tests, counting methods – assessments of conception probability based on counting forward from actual or retrospectively recalled onset of last menses, or backward from actual or anticipated onset of next menses – are more common. The validity of these methods remains largely unexplored. Based on published data on the distributions of the lengths of follicular and luteal phases, we created a sample of 58,000+ simulated cycles. We used the sample to assess the validity of counting methods. Aside from methods that count backward from a confirmed onset of next menses, validities are modest, generally ranging from about .40–.55. We offer power estimates and make recommendations for future work. We also discuss implications for interpreting past research.},
  langid = {english},
  number = {2}
}

