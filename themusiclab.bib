}

@article{Cosmides1989,
  title = {Evolutionary Psychology and the Generation of Culture, Part {{II}}: {{Case}} Study: {{A}} Computational Theory of Social Exchange},
  shorttitle = {Evolutionary Psychology and the Generation of Culture, Part {{II}}},
  author = {Cosmides, Leda and Tooby, John},
  date = {1989-01},
  journaltitle = {Ethology and Sociobiology},
