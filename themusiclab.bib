@article{Bainbridge2020,
  title = {Infants Relax in Response to Unfamiliar Foreign Lullabies},
  author = {Bainbridge, Constance M and Youngers, Julie and Bertolo, Mila and Atwood, S. and {yurdum}, lidya and Lopez, Kelsie and Xing, Feng and Martin, Alia and Mehr, Samuel A},
  date = {2020-01-18},
  journaltitle = {PsyArXiv},
  url = {https://osf.io/xcj52},
  urldate = {2020-01-21},
  abstract = {Music is a human universal [1–3] which, like nonhuman vocalizations [4,5], is characterized by acoustical forms that are predictive of its behavioral functions [1,6]. Adult listeners use this fact to accurately distinguish between unfamiliar lullabies and love songs [7], dance songs, and healing songs [6], and so on, on the basis of those songs’ musical features alone [1]. This could be attributable to adults’ extensive musical experience, however, as opposed to the basic design of the human mind. Here we show that American infants (N = 144) relax in response to lullabies from eight small-scale societies, relative to matched songs from other small-scale societies, as measured by heart rate and electrodermal activity. These results were consistent throughout the first year of life, suggesting the relaxation response is not a function of infants’ rich musical experiences [8–10]. Infants showed no visual preferences for the animated characters producing the songs, but they attended more to the lullabies, as evidenced by less blinking during the singing. Moreover, the infants’ parents chose foreign lullabies as the songs that they themselves would use to calm their fussy infant, much more often than other foreign songs. Together, these findings raise the possibility that links between form and function in music are innately specified, in contrast to features of music perception that are culturally determined [3,11].}
}

