  journaltitle = {Journal of Experimental Psychology: Human Learning and Memory},
  shortjournal = {J. Exp. Psychol. [Hum. Learn.]},
  volume = {3},
  pages = {60--67},
  issn = {0096-1515(Print)},
  doi = {10.1037/0278-7393.3.1.60},
  abstract = {In 3 experiments, sequences of 7 tones were presented to a total of 80 musically trained and untrained high school and college students, and recognition memory for individual tones of each sequence was tested under varying degrees of context. With no context, the test required recognition of a tone isolated from the sequence; with full context, the tone to be recognized was embedded in the original sequence. Results demonstrate that recognition memory was far more accurate under full than under no-context conditions and that the superiority was not wholly attributable to the serial position information or the order information provided by the full context. It is suggested that in addition to the processing of limited information for the pitch of isolated tones, pattern (or relational) information is abstracted from a tone sequence and is retrieved in the presence of full context. (20 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {Contextual Associations,Memory,mila to read,Music Education,Pitch Discrimination,Recognition (Learning),tonic},
  number = {1}
}

