@article{VanSchaik1983,
  title = {Why Are Diurnal Primates Living in Groups?},
  author = {Van Schaik, C. P.},
  date = {1983},
  journaltitle = {Behaviour},
  volume = {87},
  pages = {120--144},
  issn = {0005-7959},
  url = {https://www.jstor.org/stable/4534297},
  urldate = {2019-09-12},
  abstract = {There are two main competing theories on the evolution of group living in diurnal nonhuman primates. The first theory claims that predation avoidance favours group living, whereas there are only disadvantages to feeding in a group and feeding competition increases with group size. The second theory claims that there is a feeding advantage to group living deriving from communal defence of high-quality food patches and that predation is not important. These theories have not yet been rigorously tested. In this paper a critical test is proposed: the theories differ in the predicted relationship between a female's birth rate and the size of the group in which she lives (Fig. 1). An additional test is concerned with the predicted relationship between population density relative to food availability and average group size. Finally, a critical test is proposed of the hypothesis that increasing group size should lead to reduced predation risk by comparing demographic patterns between areas where predators are still present and where they have disappeared. A total of 23 data sets on 13 species were extracted from the literature and supplemented with four unpublished data sets. In all three tests the results provide strong support for the predation-feeding competition theory and are clearly unfavourable for the theory postulating feeding advantages to group living. Such feeding advantages may, however, gain prominence under some conditions. /// Es gibt zwei Theorien über die Evolution des Lebens in Gruppen bei tagaktiven nichthumanen Primaten. Die eine Theorie behauptet dass die Vermeidung von Raubfeinden der grundsätzliche förderende Faktor ist. Der Futtererwerb würde in erster Instanz nur nachteilig beeinflusst durch das Gruppenleben; die Futterkonkurrenz würde zunehmen mit wachsender Gruppengrösse. Die andere Theorie behauptet dass der Futtererwerb in Gruppen vorteilhafter verläuft, in erster Instanz dadurch dass Gruppenmitglieder sich Futterquellen hoher Güte versichern können durch deren gemeinschafliche Verteidigung. Dagegen käme der Raubfeindvermeidung in dieser Hinsicht keine Bedeutung zu. Diese Theorien sind bis jetzt noch nicht tüchtig geprüft worden. Deswegen führen wir hier eine kritische Probe vor. Die Theorien underscheiden sich bezüglich der überprüfbaren Voraussage über die Beziehung zwischen der Geburtszahl eines Weibchens und der Grösse der Gruppe in der sie lebt (Fig. 1). Eine weitere Probe betrifft die unterschiedlichen Voraussagen über die Relation zwischen Populationsdichte in Beziehung zur Futtervorrätigkeit und der mittleren Gruppengrösse. Zuletzt wird die Hypothese geprüft dass mit zunehmender Gruppengrösse die Predationsgefahr abnimmt. Insgesammt 23 Datenbestände von 13 Arten, dem Schrifttum entnommen, und vier Bestände, die von unseren Untersuchungen an Javaneraffen stammen, wurden in Hinsicht auf die genannten Fragen analysiert. Alle drei vorgestellten Proben unterstützen die Theorie, in der Raubfeindvermeidung als positiver Faktor und Futterkonkurrenz als negativer Faktor gesehen wird. Sie widersprechen der Theorie, in der Selektionsvorteil für das Gruppenleben dem besseren Futtererwerb zugeschrieben wird. Dieses schliesst aber nicht aus dass Vorteile für den Futtererwerb unter Umständen aus dem Gruppenleben hervorgehen können.},
  number = {1/2}
}

