@article{Hewlett1998,
  title = {Culture and Early Infancy among Central {{African}} Foragers and Farmers},
  author = {Hewlett, Barry S. and Lamb, Michael E. and Shannon, Donald and Leyendecker, Birgit and Schölmerich, Axel},
  date = {1998-07},
  journaltitle = {Developmental Psychology},
  shortjournal = {Developmental Psychology},
  volume = {34},
  pages = {653--661},
  issn = {0012-1649},
  doi = {10.1037/0012-1649.34.4.653},
  url = {http://ezp-prod1.hul.harvard.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=pdh&AN=1998-04482-006&site=ehost-live&scope=site},
  urldate = {2014-11-17},
  abstract = {[Correction Notice: An erratum for this article was reported in Vol 34(5) of Developmental Psychology (see record 2008-09592-001). In this article, part of a sentence in the Results section on page 656 was inadvertently omitted during manuscript preparation. The correct sentence is provided in the erratum.] Everyday infant experiences among the Aka hunter -gatherers and the neighboring Ngandu farmers were observed and compared. Twenty Aka and 21 Ngandu 3- to 4-month-olds and 20 Aka and 20 Ngandu 9- to 10-month-olds were observed for 3 hr on each of 4 days so that all 12 daylight hr were covered. The Aka infants were more likely to be held, fed, and asleep or drowsy, whereas Ngandu infants were more likely to be alone and to fuss or cry, smile, vocalize, or play. The amount of crying, soothing, feeding, and sleeping declined over time in both groups. Distal social interaction increased over time among the Ngandu but not among the Aka. Despite striking cultural differences on many variables, however, functional context systematically affected the relative prominence of the infants' behavior in both cultural groups. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (journal abstract)},
