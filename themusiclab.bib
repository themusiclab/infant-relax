@article{Schober2016,
  title = {Listeners' and {{Performers}}' {{Shared Understanding}} of {{Jazz Improvisations}}},
  author = {Schober, Michael F. and Spiro, Neta},
  date = {2016-11-02},
  journaltitle = {Frontiers in Psychology},
