  eprinttype = {pmid},
  file = {/Users/samuelmehr/Zotero/storage/APPTS2L3/Saffran et al. - 1996 - Statistical Learning by 8-Month-Old Infants.pdf;/Users/samuelmehr/Zotero/storage/YJVBTTRH/tab-pdf.html},
  langid = {english},
  number = {5294}
}

