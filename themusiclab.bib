  author = {Armstrong, Bridget and Covington, Lauren B. and Hager, Erin R. and Black, Maureen M.},
  date = {2019-10},
  journaltitle = {Sleep Health},
  shortjournal = {Sleep Health},
  volume = {5},
  pages = {459--465},
  issn = {23527218},
