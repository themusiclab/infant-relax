@article{Rybanska2017,
  title = {Rituals {{Improve Children}}'s {{Ability}} to {{Delay Gratification}}},
  author = {Rybanska, Veronika and McKay, Ryan and Jong, Jonathan and Whitehouse, Harvey},
  date = {2017-02-01},
  journaltitle = {Child Development},
  shortjournal = {Child Dev},
  pages = {n/a-n/a},
  issn = {1467-8624},
  doi = {10.1111/cdev.12762},
  url = {http://onlinelibrary.wiley.com/doi/10.1111/cdev.12762/abstract},
  urldate = {2017-02-21},
  abstract = {To be accepted into social groups, individuals must internalize and reproduce appropriate group conventions, such as rituals. The copying of such rigid and socially stipulated behavioral sequences places heavy demands on executive function. Given previous research showing that challenging executive functioning improves it, it was hypothesized that engagement in ritualistic behaviors improves children's executive functioning, in turn improving their ability to delay gratification. A 3-month circle time games intervention with 210 schoolchildren (Mage~=~7.78~years, SD~=~1.47) in two contrasting cultural environments (Slovakia and Vanuatu) was conducted. The intervention improved children's executive function and in turn their ability to delay gratification. Moreover, these effects were amplified when the intervention task was imbued with ritual, rather than instrumental, cues.},
  langid = {english}
}

@article{Rzeszutek2012,
  title = {The Structure of Cross-Cultural Musical Diversity},
  author = {Rzeszutek, Tom and Savage, Patrick E. and Brown, Steven},
  date = {2012-04-22},
  journaltitle = {Proceedings of the Royal Society of London B: Biological Sciences},
  shortjournal = {Proc. R. Soc. Lond. B Biol. Sci.},
  volume = {279},
  pages = {1606--1612},
  issn = {0962-8452, 1471-2954},
  doi = {10.1098/rspb.2011.1750},
  url = {http://rspb.royalsocietypublishing.org/content/279/1733/1606},
  urldate = {2015-05-11},
  abstract = {Human cultural traits, such as languages, musics, rituals and material objects, vary widely across cultures. However, the majority of comparative analyses of human cultural diversity focus on between-culture variation without consideration for within-culture variation. In contrast, biological approaches to genetic diversity, such as the analysis of molecular variance (AMOVA) framework, partition genetic diversity into both within- and between-population components. We attempt here for the first time to quantify both components of cultural diversity by applying the AMOVA model to music. By employing this approach with 421 traditional songs from 16 Austronesian-speaking populations, we show that the vast majority of musical variability is due to differences within populations rather than differences between. This demonstrates a striking parallel to the structure of genetic diversity in humans. A neighbour-net analysis of pairwise population musical divergence shows a large amount of reticulation, indicating the pervasive occurrence of borrowing and/or convergent evolution of musical features across populations.},
  eprint = {22072606},
  eprinttype = {pmid},
  langid = {english},
  number = {1733}
}

@article{Saffran1996,
