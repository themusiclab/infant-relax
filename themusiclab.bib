@article{Pearce2006,
  title = {Expectation in {{Melody}}: {{The Influence}} of {{Context}} and {{Learning}}},
  shorttitle = {Expectation in {{Melody}}},
  author = {Pearce, Marcus T. and Wiggins, Geraint A.},
  date = {2006},
  journaltitle = {Music Perception: An Interdisciplinary Journal},
