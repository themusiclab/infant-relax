  eprinttype = {pubmed},
  keywords = {Acoustic Stimulation: methods,Age Factors,Aptitude,Aptitude: physiology,Brain,Brain: growth & development,Child,Female,Humans,Male,music,music training/ed,Neuronal Plasticity,Neuronal Plasticity: physiology,Pitch Discrimination,Pitch Discrimination: physiology,Reading,Speech,Speech: physiology},
