@article{Dearn2014,
  title = {The {{Effect}} of {{Maternal Presence}} on {{Premature Infant Response}} to {{Recorded Music}}},
  author = {Dearn, Trish and Shoemark, Helen},
  date = {2014-03-01},
  journaltitle = {Journal of Obstetric, Gynecologic, \& Neonatal Nursing},
  shortjournal = {Journal of Obstetric, Gynecologic, \& Neonatal Nursing},
  pages = {n/a-n/a},
  issn = {1552-6909},
