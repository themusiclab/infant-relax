@article{Musolf2010,
  ids = {Musolf2010a},
  title = {Ultrasonic Courtship Vocalizations in Wild House Mice, {{Mus}} Musculus Musculus},
  author = {Musolf, Kerstin and Hoffmann, Frauke and Penn, Dustin J.},
  date = {2010-03},
  journaltitle = {Animal Behaviour},
  shortjournal = {Animal Behaviour},
  volume = {79},
