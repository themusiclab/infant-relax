@article{Rautava1995,
  title = {Infantile {{Colic}}: {{Child}} and {{Family Three Years Later}}},
  shorttitle = {Infantile {{Colic}}},
  author = {Rautava, Päivi and Lehtonen, Liisa and Helenius, Hans and Sillanpää, Matti},
  date = {1995-07-01},
  journaltitle = {Pediatrics},
  volume = {96},
