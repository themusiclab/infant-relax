@article{Soley2020,
  title = {Infants' Expectations about the Recipients of Infant-Directed and Adult-Directed Speech},
  author = {Soley, Gaye and Sebastian-Galles, Nuria},
  date = {2020-05-01},
  journaltitle = {Cognition},
